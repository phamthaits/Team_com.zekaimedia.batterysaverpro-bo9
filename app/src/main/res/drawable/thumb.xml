<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="oval" android:useLevel="false" android:visible="true">
            <solid android:color="@color/thumb_uncheck" />
            <size android:width="@dimen/_14sdp" android:height="@dimen/_14sdp" />
            <stroke android:width="@dimen/_5sdp" android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="oval" android:useLevel="false" android:visible="true">
            <solid android:color="@color/thumb_check" />
            <size android:width="@dimen/_14sdp" android:height="@dimen/_14sdp" />
            <stroke android:width="@dimen/_5sdp" android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>